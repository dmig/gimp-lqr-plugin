{
    "configurations": [
        {
            "name": "GIMP plug-in",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/gimp-2.0",
                "/usr/include/glib-2.0",
                "/usr/lib/x86_64-linux-gnu/gtk-2.0/include",
                "/usr/lib/x86_64-linux-gnu/glib-2.0/include",
                "/usr/include/cairo",
                "/usr/include/gegl-0.4",
                "/usr/include/babl-0.1",
                "/usr/include/gdk-pixbuf-2.0",
                "/usr/include/gtk-2.0",
                "/usr/include/pango-1.0",
                "/usr/include/atk-1.0",
                "/usr/include/lqr-1"
            ],
            "defines": [
                "LOCALEDIR=\"${env:HOME}/.local/share/locale\"",
                "PLUGIN_DATADIR=\"${env:HOME}/.local/share/gimp-lqr-plugin\""
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}
